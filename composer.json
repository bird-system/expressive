{
  "name": "birdsystem/expressive",
  "require": {
    "php": "^7.0",
    "zendframework/zend-mvc-console": "^1",
    "zendframework/zend-code": "^3",
    "zendframework/zend-cache": "^2",
    "zendframework/zend-db": "^2.8.2",
    "zendframework/zend-authentication": "^2.5.3",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-session": "^2.7.1",
    "zendframework/zend-expressive": "^2.0.2",
    "zendframework/zend-expressive-helpers": "^4.0",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-i18n": "^2.7.4",
    "zendframework/zend-console": "^2.6.0",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-expressive-zendrouter": "^2.0.1",
    "mattketmo/camel": "1.1.0",
    "crisu83/php-conversion": "dev-master",
    "zendframework/zend-json": "^3.0.0",
    "theseer/fdomdocument": "1.6.*",
    "sebastian/finder-facade": "~1.1",
    "monolog/monolog": "^1.23.0",
    "enlitepro/enlite-monolog": "^2.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.8 || ^5.7.15",
    "squizlabs/php_codesniffer": "^2.8.1",
    "zfcampus/zf-development-mode": "^3.1",
    "filp/whoops": "^2.1.7",
    "fzaninotto/Faker": "dev-master",
    "bramus/monolog-colored-line-formatter": "~2"
  },
  "autoload": {
    "psr-4": {
      "BS\\": "module/BS/src",
      "CLI\\": "module/CLI/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BS\\Tests\\": "module/BS/tests",
      "CLI\\Tests\\": "module/CLI/tests"
    }
  }
}
